@model MVCCV.Models.Class.Class1
@{
    Layout = null;

}

<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>MVCCV</title>

    <!-- Bootstrap core CSS -->
    <link href="~/startbootstrap-resume-gh-pages/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <!-- Custom fonts for this template -->
    <link href="https://fonts.googleapis.com/css?family=Saira+Extra+Condensed:500,700" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Muli:400,400i,800,800i" rel="stylesheet">
    <link href="~/startbootstrap-resume-gh-pages/vendor/fontawesome-free/css/all.min.css" rel="stylesheet">

    <!-- Custom styles for this template -->
    <link href="~/startbootstrap-resume-gh-pages/css/resume.min.css" rel="stylesheet" />
    <link href="~/startbootstrap-resume-gh-pages/css/dark.css" rel="stylesheet" />
</head>
<body id="page-top">
   
    <nav class="navbar navbar-expand-lg navbar-dark bg-primary fixed-top" id="sideNav">
        <div>
            <input type="checkbox" class="checkbox" id="chk" />
            <label class="label" for="chk">
                <i class="fas fa-moon"></i>
                <i class="fas fa-sun"></i>
                <div class="ball"></div>

            </label>
        </div>
        <a class="navbar-brand js-scroll-trigger" href="#page-top">
            <span class="d-block d-lg-none">Maftun Hashimli</span>
            <span class="d-none d-lg-block">
                <img class="img-fluid img-profile rounded-circle mx-auto mb-2" src="~/startbootstrap-resume-gh-pages/img/alt_pic.jpg" alt="">
            </span>
        </a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link js-scroll-trigger" href="#about">About</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link js-scroll-trigger" href="#experience">Experience</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link js-scroll-trigger" href="#education">Education</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link js-scroll-trigger" href="#skills">Skills</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link js-scroll-trigger" href="#interests">Interests</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link js-scroll-trigger" href="#courses">Courses</a>

                </li>
            </ul>
        </div>
    </nav>

    <div class="container-fluid p-0">

        <section class="resume-section p-3 p-lg-5 d-flex d-column" id="about">
            @foreach (var variables in Model.VarAbout)
            {
                <div class="my-auto">

                    <h1 class="mb-0">
                        @variables.NAME
                        <span class="text-primary">@variables.SURNAME</span>
                    </h1>
                    <div class="subheading mb-5">
                        @variables.ADDRESS· @variables.PHONE ·
                        <a href="mailto:maftunhashimli@gmail.com">@variables.MAIL</a>
                    </div>
                    <p class="lead mb-5">@variables.ABOUT</p>
                    <div class="social-icons">
                        <a href="https://www.linkedin.com/in/maftunhashimli/">
                            <i class="fab fa-linkedin-in"></i>
                        </a>
                        <a href="https://github.com/MeftunH">
                            <i class="fab fa-github"></i>
                        </a>
                        <a href="mailto:maftunhashimli@gmail.com">

                            <i class="fab fa-mail-reply-all">M</i>
                        </a>

                    </div>
                </div>
            }
        </section>

        <hr class="m-0">

        <section class="resume-section p-3 p-lg-5 d-flex flex-column" id="experience">

            <div class="my-auto">
                <h2 class="mb-5">Experience</h2>
                @foreach (var variable in Model.VarExperience)
                {
                    <div class="resume-item d-flex flex-column flex-md-row mb-5">

                        <div class="resume-content mr-auto">
                            <h3 class="mb-0">@variable.TITLE</h3>
                            <div class="subheading mb-3">@variable.SUBTITLE</div>
                            <p>@variable.DETAILS</p>
                        </div>
                        <div class="resume-date text-md-right">
                            <span class="text-primary">@variable.DATE</span>
                        </div>

                    </div>
                }
            </div>

        </section>

        <hr class="m-0">

        <section class="resume-section p-3 p-lg-5 d-flex flex-column" id="education">
            <div class="my-auto">
                <h2 class="mb-5">Education</h2>
                @foreach (var ed in Model.VarEducation)
                {
                    <div class="resume-item d-flex flex-column flex-md-row mb-5">
                        <div class="resume-content mr-auto">
                            <h3 class="mb-0">@ed.TITLE</h3>
                            <div class="subheading mb-3">@ed.SUBTITLE</div>
                            <div>@ed.DEPARTMENT</div>
                            <p>@ed.GPA</p>
                        </div>
                        <div class="resume-date text-md-right">
                            <span class="text-primary">GPA: @ed.Date</span>
                        </div>
                    </div>
                }
            </div>
        </section>

        <hr class="m-0">

        <section class="resume-section p-3 p-lg-5 d-flex flex-column" id="skills">
            <div class="my-auto">
                <h2 class="mb-5">Skills</h2>

                <div class="subheading mb-3">Programming Languages &amp; Tools</div>
                <ul class="list-inline dev-icons">
                    <li class="list-inline-item">
                        <i></i>
                        <i class="fab fa-html5"></i>
                    </li>
                    <li class="list-inline-item">
                        <i class="fab fa-css3-alt"></i>
                    </li>
                    <li class="list-inline-item">
                        <i class="">Bootstrap</i>
                    </li>
                    <li class="list-inline-item">
                        <i class="fab fa-php"></i>
                    </li>
                    <li class="list-inline-item">
                        <i></i>
                        <i class="dropleft">C#</i>
                    </li>
                    <li class="list-inline-item">

                        <i class="">C++</i>
                    </li>
                    <li class="list-inline-item">
                        <i class="">C</i>
                    </li>

                    <li class="list-inline-item">
                        <i class="">.Net Core</i>
                    </li>
                    <li class="list-inline-item">
                        <i class="">Asp.NET</i>
                    </li>
                    <li class="list-inline-item">
                        <i class="fab fa-js-square"></i>
                    </li>
                    <li class="list-inline-item">

                    </li>
                    <li class="list-inline-item">
                        <i class="fab">Database Management</i>
                    </li>
                    <li class="list-inline-item">
                        <i class="fab fa-java"></i>
                    </li>
                    <li class="list-inline-item">
                        <i class="fab"></i>
                    </li>
                </ul>

                <div class="subheading mb-3">Workflow</div>
                @foreach (var x in Model.VarSkills)
                {
                    <ul class="fa-ul mb-0">
                        <li>
                            <i class="fa-li fa fa-check"></i>
                            @x.SKILLS
                        </li>
                    </ul>
                }
            </div>

        </section>

        <hr class="m-0">

        <section class="resume-section p-3 p-lg-5 d-flex flex-column" id="interests">

            <div class="my-auto">
                <h2 class="mb-5">Interests</h2>
                @foreach (var x in Model.VarInterests)
                {
                    <p>@x.INTEREST</p>
                }
            </div>
        </section>

        <hr class="m-0">

        <section class="resume-section p-3 p-lg-5 d-flex flex-column" id="courses">
            <div class="my-auto">
                <h2 class="mb-5">Courses</h2>
                @foreach (var x in Model.VarCourses)
                {
                    <ul class="fa-ul mb-0">
                        <li class="list-inline-item">
                            <i class="fab fa-star"></i>

                        </li> @x.COURSE
                    </ul>
                }
            </div>
        </section>

    </div>

    <!-- Bootstrap core JavaScript -->
    <script src="~/startbootstrap-resume-gh-pages/vendor/jquery/jquery.min.js"></script>
    <script src="~/startbootstrap-resume-gh-pages/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

    <!-- Plugin JavaScript -->
    <script src="~/startbootstrap-resume-gh-pages/vendor/jquery-easing/jquery.easing.min.js"></script>

    <!-- Custom scripts for this template -->
    <script src="~/startbootstrap-resume-gh-pages/js/resume.min.js"></script>
    <script src="~/startbootstrap-resume-gh-pages/js/dark.js"></script>
</body>

</html>

