@model MVCCV.Models.Entity.TBL_EXPERIENCE
@{
    ViewBag.Title = "GetExperience";
    Layout = "~/Views/Shared/_Main_Layout.cshtml";
}
@using (Html.BeginForm("UPDATE", "Experiences", FormMethod.Post))
{
    <div class="form-group">
        @Html.LabelFor(m => m.ID)
        @Html.TextBoxFor(m => m.ID, new { @class = "form-control" })
        <br />
        @Html.LabelFor(m => m.TITLE)
        @Html.TextBoxFor(m => m.TITLE, new { @class = "form-control" })
        <br />
        @Html.LabelFor(m => m.SUBTITLE)
        @Html.TextBoxFor(m => m.SUBTITLE, new { @class = "form-control" })
        <br />
        @Html.LabelFor(m => m.DATE)
        @Html.TextBoxFor(m => m.DATE, new { @class = "form-control" })
        <br />
        @Html.LabelFor(m => m.DETAILS)
        @Html.TextBoxFor(m => m.DETAILS, new { @class = "form-control" })
        <br />

        <button class="btn btn-primary">UPDATE</button>
    </div>

}


